							exec -
							exec - !
							exec - sed \
							exec - -e '/^b_[a-z_][a-z_0-9]*(/!d' \
							exec - -e 's/^b_//' \
							exec - -e 's/(.*//' \
							exec - -e 's/.*/extern int	b_&(int, char**, Shbltin_t*);/' \
